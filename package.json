{
  "name": "typescript-starter-project",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start-development": "npm run chromium-remote-debugging & npm run live-server & npm run compile-typescript-watch",
    "compile-typescript": "tsc",
    "compile-typescript-watch": "tsc --watch",
    "test": "mocha --reporter min --require esm --require ./test/setup-jsdom.js",
    "test-watch": "mocha --watch --reporter min --require esm --require ./test/setup-jsdom.js",
    "live-server": "live-server --browser=chromium-browser --ignore=**/*.ts,**/*.map",
    "chromium-remote-debugging": "chromium-browser --remote-debugging-port=9222",
    "build-bundle": "rollup --config"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.9.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.8",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "esm": "^3.2.22",
    "jsdom": "^15.0.0",
    "live-server": "^1.2.1",
    "mocha": "^6.1.4",
    "rollup": "^1.10.1",
    "typescript": "^3.4.5"
  },
  "prettier": {
    "singleQuote": true,
    "proseWrap": "always",
    "trailingComma": "all"
  }
}
